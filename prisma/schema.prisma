datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

model Users {
  id       Int      @id @default(autoincrement())
  email    String   @unique
  name     String?
}
model TimeLogs {
  id            Int    @id @default(autoincrement())
  email         String
  day           String
  startTime     Int
  endTime       Int?
  pausaInicio   Int?
  pausaFim      Int?
  pauseDuration Int?
  totalDuration Int?
  obsStart      String?
  obsEnd        String?
}
