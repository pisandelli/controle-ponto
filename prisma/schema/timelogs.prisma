model TimeLogs {
  id            Int    @id @default(autoincrement())
  email         String
  today         String
  month         String
  year          String
  startTime     Int
  endTime       Int?
  pausaInicio   Int?
  pausaFim      Int?
  pauseDuration Int?
  totalDuration Int?
  obsStart      String?
  obsEnd        String?
}